{
  "version": 1,
  "categories": {
    "git": {
      "icon": "git",
      "description": "Git version control commands",
      "commands": [
        {
          "command": "/git status",
          "description": "Show working tree status",
          "action": "insert_text",
          "value": "git status"
        },
        {
          "command": "/git add",
          "description": "Stage all changes",
          "action": "insert_text",
          "value": "git add ."
        },
        {
          "command": "/git commit",
          "description": "Commit with message",
          "action": "insert_template",
          "value": "git commit -m \"${1:message}\""
        },
        {
          "command": "/git push",
          "description": "Push to remote",
          "action": "insert_text",
          "value": "git push"
        },
        {
          "command": "/git pull",
          "description": "Pull from remote",
          "action": "insert_text",
          "value": "git pull"
        },
        {
          "command": "/git log",
          "description": "Show commit log (oneline)",
          "action": "insert_text",
          "value": "git log --oneline -20"
        },
        {
          "command": "/git diff",
          "description": "Show changes",
          "action": "insert_text",
          "value": "git diff"
        },
        {
          "command": "/git branch",
          "description": "List branches",
          "action": "insert_text",
          "value": "git branch -a"
        },
        {
          "command": "/git checkout",
          "description": "Switch branch",
          "action": "insert_template",
          "value": "git checkout ${1:branch}"
        },
        {
          "command": "/git stash",
          "description": "Stash changes",
          "action": "insert_text",
          "value": "git stash"
        }
      ]
    },
